{
    "title": "Redridge Digital",
    "name": "Bootstrap - template with php post",
    "version": "5.0.8",
    "scripts": {
        "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets",
        "build:assets": "node scripts/build-assets.js",
        "build:pug": "node scripts/build-pug.js",
        "build:scripts": "node scripts/build-scripts.js",
        "build:scss": "node scripts/build-scss.js",
        "clean": "node scripts/clean.js",
        "start": "npm run build && node scripts/start.js",
        "start:debug": "npm run build && node scripts/start-debug.js"
    },
    "description": "A simple business HTML landing page built for Red Ridge Digital",
    "keywords": [
        "css",
        "sass",
        "html",
        "responsive",
        "theme",
        "template"
    ],
    "homepage": "https://redridgedigital.com",
    "bugs": {
        "url": "https://support.redridgedigital.com.com",
        "email": "support@redridge.com"
    },
    "license": "MIT",
    "author": "Will Sanchez",
    "contributors": [
        "David Miller (https://davidmiller.io/)"
    ],

    "dependencies": {
        "bootstrap": "5.1.3"
    },
    "devDependencies": {
        "autoprefixer": "10.4.4",
        "browser-sync": "2.27.9",
        "chokidar": "3.5.3",
        "concurrently": "6.3.0",
        "postcss": "8.4.12",
        "prettier": "2.6.0",
        "pug": "3.0.2",
        "sass": "1.49.9",
        "shelljs": "0.8.5",
        "upath": "2.0.1"
    }
